[ignore]
.*/node_modules/babel*
.*/node_modules/webpack*
.*/node_modules/karma*
.*/node_modules/react/.*
.*/node_modules/fbjs/.*
.*/app-legacy/.*

[libs]
./lib

[options]
module.system=node
module.name_mapper='^.*\(css\)$' -> 'empty/object'
module.name_mapper='^\(shared.*\)' -> './\1'
module.name_mapper='^\(shared.*\)' -> '../\1'
module.name_mapper='^\(shared.*\)' -> '../../\1'
module.name_mapper='^\(shared.*\)' -> '../../../\1'

suppress_comment= \\(.\\|\n\\)*\\$FlowFixMe
